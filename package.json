{
	"name": "opener",
	"version": "2.2.6",
	"description": "Open links in new/existing tabs by default. Can open PDFs and other file formats in System Apps if desired.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian-plugin",
		"obsidian",
		"opener",
		"tab",
		"tabs",
		"new tab",
		"pdf"
	],
	"author": "LukeMT, Aidan Gibson",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/lukemt/obsidian-opener.git"
	},
	"bugs": {
		"url": "https://github.com/lukemt/obsidian-opener/issues"
	},
	"homepage": "https://github.com/lukemt/obsidian-opener#readme",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"monkey-around": "^2.3.0"
	}
}
